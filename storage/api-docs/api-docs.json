{
    "openapi": "3.0.0",
    "info": {
        "title": "用户模块",
        "version": "0.1"
    },
    "paths": {
        "/api/user": {
            "get": {
                "summary": "查询用户资料",
                "operationId": "App\\Http\\Controllers\\UserController::show",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "schema": {
                            "type": "int"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "success"
                    }
                }
            },
            "put": {
                "summary": "修改用户资料",
                "operationId": "App\\Http\\Controllers\\UserController::update",
                "parameters": [
                    {
                        "name": "Accept",
                        "in": "header",
                        "content": "application/json"
                    }
                ],
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "id": {
                                        "type": "int"
                                    },
                                    "name": {
                                        "type": "string"
                                    }
                                },
                                "type": "object",
                                "example": {
                                    "id": 10,
                                    "name": "Jessica Smith"
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": ""
                    }
                }
            }
        },
        "/api/user/invites": {
            "get": {
                "summary": "用户邀请列表",
                "operationId": "App\\Http\\Controllers\\UserController::invites",
                "responses": {
                    "200": {
                        "description": "success"
                    }
                }
            }
        }
    }
}