        "/api/stores/coupon/publish": {
            "put": {
                "summary": "商家生成优惠券",
                "operationId": "App\\Http\\Controllers\\Store\\CouponController::publish",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "coupon_name": {
                                        "description": "优惠券名称",
                                        "type": "string"
                                    },
                                    "coupon_explain": {
                                        "description": "优惠券说明",
                                        "type": "string"
                                    },
