{
    "openapi": "3.0.0",
    "info": {
        "title": "用户模块",
        "version": "0.1"
    },
    "paths": {
        "/api/stores": {
            "get": {
                "summary": "首页商家列表",
                "operationId": "App\\Http\\Controllers\\StoreController::index",
                "responses": {
                    "200": {
                        "description": "success"
                    }
                }
            }
        },
        "/api/store/search": {
            "get": {
                "summary": "搜索商家",
                "operationId": "App\\Http\\Controllers\\StoreController::search",
                "responses": {
                    "200": {
                        "description": "success"
                    }
                }
            }
        },
        "/api/store/filter": {
            "get": {
                "summary": "商家筛选",
                "operationId": "App\\Http\\Controllers\\StoreController::filter",
                "responses": {
                    "200": {
                        "description": "success"
                    }
                }
            }
        },
        "/api/stores/1": {
            "get": {
                "summary": "商家详情",
                "operationId": "App\\Http\\Controllers\\StoreController::show",
                "responses": {
                    "200": {
                        "description": "success"
                    }
                }
            }
        },
        "/api/user": {
            "get": {
                "summary": "查询用户资料",
                "operationId": "App\\Http\\Controllers\\UserController::show",
                "responses": {
                    "200": {
                        "description": "success"
                    }
                }
            },
            "put": {
                "summary": "修改用户资料",
                "operationId": "App\\Http\\Controllers\\UserController::update",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "nickname": {
                                        "type": "string"
                                    },
                                    "phone": {
                                        "type": "string"
                                    }
                                },
                                "type": "object",
                                "example": {
                                    "nickname": "Jessica Smith",
                                    "phone": "13012341234"
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "{code:0}"
                    }
                }
            }
        },
        "/api/user/invites": {
            "get": {
                "summary": "用户邀请列表",
                "operationId": "App\\Http\\Controllers\\UserController::invites",
                "responses": {
                    "200": {
                        "description": "success"
                    }
                }
            }
        }
    }
}