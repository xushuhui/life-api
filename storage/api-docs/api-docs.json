                                        "description": "优惠券类型：1.普通券；2.联盟券；3.次卡券；4.储值券",
                                        "type": "number"
                                    },
                                    "end_time": {
                                        "description": "结束时间：2020-05-05",
                                        "type": "date"
                                    },
                                    "total_num": {
                                        "description": "总数",
                                        "type": "int"
                                    },
                                    "user_num": {
                                        "description": "每个人可使用的次数",
                                        "type": "int"
                                    },
                                    "is_rec": {
                                        "description": "是否推荐：0.否；1.是",
                                        "type": "int"
                                    },
                                    "use_notice": {
                                        "description": "使用须知",
                                        "type": "string"
                                    },
                                    "careful_matter": {
                                        "description": "注意事项",
                                        "type": "string"
                                    },
                                    "recharge_amount": {
                                        "description": "充值金额",
                                        "type": "float"
                                    },
                                    "give_amount": {
                                        "description": "赠送金额",
                                        "type": "float"
                                    }
                                },
                                "type": "object"
